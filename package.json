{
  "name": "@firstclasspostcodes/sw14",
  "version": "0.0.1",
  "description": "CSS-in-JS based theme library for the SW14 components library",
  "main": "commonjs/index.js",
  "module": "esm/index.js",
  "scripts": {
    "build": "npm run build-icons && npm run build-commonjs && npm run build-esm",
    "build-icons": "node lib/components/Icon/__compile.js",
    "build-commonjs": "BABEL_ENV=commonjs babel lib --out-dir commonjs --copy-files --ignore '**/test' --source-maps inline",
    "build-esm": "BABEL_ENV=esm babel lib --out-dir esm --copy-files --ignore '**/test' --source-maps inline",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "commit": "npx git-cz",
    "lint": "eslint . --ext .js --ext .jsx",
    "pretest": "npm run lint",
    "storybook": "start-storybook -p 6006",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstclasspostcodes/sw14.git"
  },
  "keywords": [],
  "author": "Firstclasspostcodes <support@firstclasspostcodes.com> (https://firstclasspostcodes.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/firstclasspostcodes/sw14/issues"
  },
  "homepage": "https://github.com/firstclasspostcodes/sw14#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.13",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-docs": "^6.1.17",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/addon-viewport": "^6.1.17",
    "@storybook/addons": "^6.1.17",
    "@storybook/react": "^6.1.17",
    "@storybook/storybook-deployer": "^2.8.7",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-runtime": "^6.26.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "git-cz": "^4.7.6",
    "glob": "^7.1.6",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "pascalcase": "^1.0.0",
    "prettier": "^1.19.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "dependencies": {
    "param-case": "^3.0.4",
    "polished": "^4.1.0",
    "prop-types": "^15.7.2",
    "react-cookie": "^4.0.3",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^8.31.0",
    "react-syntax-highlighter": "^15.4.3",
    "styled-components": "^5.2.1",
    "styled-components-breakpoint": "^3.0.0-preview.20",
    "styled-components-grid": "^2.2.2",
    "styled-components-spacing": "^3.1.1",
    "tinycolor2": "^1.4.2"
  }
}
