{
  "name": "@firstclasspostcodes/sw14",
  "version": "0.0.1",
  "description": "CSS-in-JS based theme library for the SW14 components library",
  "main": "commonjs/index.js",
  "module": "esm/index.js",
  "scripts": {
    "build": "npm run build-icons && npm run build-commonjs && npm run build-esm",
    "build-icons": "node lib/components/Icon/__compile.js",
    "build-commonjs": "BABEL_ENV=commonjs babel lib --out-dir commonjs --copy-files --ignore '**/test' --source-maps inline",
    "build-esm": "BABEL_ENV=esm babel lib --out-dir esm --copy-files --ignore '**/test' --source-maps inline",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "commit": "npx git-cz",
    "lint": "eslint . --ext .js --ext .jsx",
    "pretest": "npm run lint",
    "storybook": "start-storybook -p 6006",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstclasspostcodes/sw14.git"
  },
  "keywords": [],
  "author": "Firstclasspostcodes <support@firstclasspostcodes.com> (https://firstclasspostcodes.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/firstclasspostcodes/sw14/issues"
  },
  "homepage": "https://github.com/firstclasspostcodes/sw14#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-angular": "^9.1.1",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^6.0.6",
    "@storybook/addon-links": "^6.0.6",
    "@storybook/addon-viewport": "^6.0.6",
    "@storybook/addons": "^6.0.6",
    "@storybook/react": "^5.3.19",
    "@storybook/storybook-deployer": "^2.8.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-runtime": "^6.26.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "git-cz": "^4.7.0",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "jest-circus": "^26.4.0",
    "pascalcase": "^1.0.0",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "param-case": "^3.0.3",
    "polished": "^3.6.5",
    "prop-types": "^15.7.2",
    "react-cookie": "^4.0.3",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^8.26.2",
    "react-syntax-highlighter": "^13.2.1",
    "styled-components": "^5.1.1",
    "styled-components-breakpoint": "^3.0.0-preview.20",
    "styled-components-grid": "^2.2.2",
    "styled-components-spacing": "^3.1.1",
    "tinycolor2": "^1.4.1"
  }
}
