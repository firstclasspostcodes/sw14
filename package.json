{
  "name": "@firstclasspostcodes/sw14",
  "version": "0.0.1",
  "description": "CSS-in-JS based theme library for the SW14 components library",
  "main": "commonjs/index.js",
  "module": "esm/index.js",
  "scripts": {
    "build": "npm run build-icons && npm run build-commonjs && npm run build-esm",
    "build-icons": "node lib/components/Icon/__compile.js",
    "build-commonjs": "BABEL_ENV=commonjs babel lib --out-dir commonjs --copy-files --ignore '**/test' --source-maps inline",
    "build-esm": "BABEL_ENV=esm babel lib --out-dir esm --copy-files --ignore '**/test' --source-maps inline",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "commit": "npx git-cz",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "storybook": "start-storybook -p 6006",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstclasspostcodes/sw14.git"
  },
  "keywords": [],
  "author": "Firstclasspostcodes <support@firstclasspostcodes.com> (https://firstclasspostcodes.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/firstclasspostcodes/sw14/issues"
  },
  "homepage": "https://github.com/firstclasspostcodes/sw14#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-docs": "^5.3.13",
    "@storybook/addon-links": "^5.3.12",
    "@storybook/addon-viewport": "^5.3.10",
    "@storybook/addons": "^5.3.12",
    "@storybook/react": "^5.3.12",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-runtime": "^6.26.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "git-cz": "^4.3.1",
    "glob": "^7.1.6",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "jest-circus": "^25.1.0",
    "pascalcase": "^1.0.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "param-case": "^3.0.3",
    "polished": "^3.4.4",
    "prop-types": "^15.7.2",
    "react-helmet": "^5.2.1",
    "react-intersection-observer": "^8.25.2",
    "styled-components": "^5.0.1",
    "styled-components-breakpoint": "^3.0.0-preview.20",
    "styled-components-grid": "^2.2.2",
    "styled-components-spacing": "^3.1.1",
    "tinycolor2": "^1.4.1"
  }
}
